<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ds.supercar.model.vehicle.CarType"
		table="supercarcartype" schema="supercar">
		<id name="cartypeid">
			<generator class="increment" />
		</id>
		<property name="cartypename" />
		<property name="cartype_desc" unique="true"/>
		
	</class>
</hibernate-mapping>