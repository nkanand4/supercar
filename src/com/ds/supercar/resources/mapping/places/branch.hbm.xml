<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ds.supercar.model.places.Branch"
		table="supercarbranch" schema="supercar">
		<id name="branchid">
			<generator class="increment" />
		</id>
		<property name="name"/>
		<property name="contact_no" />
		<property name="fax_no" />
		
	</class>
</hibernate-mapping>